{
    "manifest_version": 2,
    "name": "TakeNode",
    "description": "Use npm in the browser",
    "version": "1",
    "permissions": [
      "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "inject.js"
            ],
            "run_at": "document_start"

        }
    ],
    "web_accessible_resources": [
    "take_bundle.js"
  ]
}